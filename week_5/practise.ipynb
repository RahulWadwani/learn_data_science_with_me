    "def count_bits(x):\n",
    "    n = 0 \n",
    "    while x:\n",
    "        n += x & 1\n",
    "        x >>= 1\n",
    "    return n\n",
    "x = int(input()) \n",
    "print(count_bits(n))"
   ]
